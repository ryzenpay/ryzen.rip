server {
        listen 80 default_server;
        listen [::]:80 default_server;


        root /var/www/html;

        index index.html index.htm search.html;

        server_name ryzen.rip;

        location / {
                autoindex on;
		add_header Content-Type text/plain;
                try_files $uri $uri/ =404;
        }

}
